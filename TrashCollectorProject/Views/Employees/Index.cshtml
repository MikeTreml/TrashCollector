@model IEnumerable<TrashCollectorProject.Models.Customer>

@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<h1>View</h1>

<p>
  
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AccountBalance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PickUpDay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OneTimePickUp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SuspendStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SuspendEnd)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccountBalance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PickUpDay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OneTimePickUp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SuspendStart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SuspendEnd)
            </td>
            
            <td>
                <a asp-action="CustomerDetails" asp-route-id="@item.Id">Details</a> |
                <a asp-action="" asp-route-id="@item.Id">Completed</a>
            </td>
        </tr>
}
    </tbody>
</table>
